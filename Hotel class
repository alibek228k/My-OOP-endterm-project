package com.company;

import java.sql.Connection;
import java.sql.PreparedStatement;
import java.sql.ResultSet;
import java.sql.Statement;
import java.util.Scanner;

public class Hotel {
    Scanner scan = new Scanner(System.in);
    Statement stat = null;
    Connection con = null;
    ResultSet set = null;
    PreparedStatement pstat = null;
    String query;
    public String hotel1(){
        con = Client.get_connection();
        while (true) {
            System.out.println("Welcome to Burj Al Arab!\n" +
                    "1.Learn more about the hotel\n" +
                    "2.Book a room\n" +
                    "3.Check out the room" +
                    "4.Inquire about food at the hotel\n" +
                    "5.Find out about entertainment at the hotel\n" +
                    "6.Find out what services the hotel has\n" +
                    "7.Find out about all rooms\n" +
                    "8.Find out about room prices\n" +
                    "9.Exit.");
            int d = 0;
            d = scan.nextInt();

            switch (d) {
                case 1:
                    System.out.println("The Burj Al Arab is a luxury hotel located in the city of Dubai, United Arab Emirates.\n" +
                            "Managed by the Jumeirah hotel group, it is one of the tallest hotels in the world,\n" +
                            "although 39% of its total height is made up of non-occupiable space. \n" +
                            "Burj Al Arab stands on an artificial island 280 m (920 ft) from Jumeirah Beach\n" +
                            "and is connected to the mainland by a private curving bridge. \n" +
                            "The shape of the structure is designed to resemble the sail of a ship.\n " +
                            "It has a helipad near the roof at a height of 210 m (689 ft) above ground.");
                    break;
                case 2:
                    try {
                        query = "Select *  from first_hotel_rooms \n" +
                                "WHERE availability = true ORDER BY room_id";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        while (set.next())
                            System.out.println(set.getInt("room_id") + " "
                                    + set.getString("availability"));
                        System.out.println("Free rooms are presented above. Write the id of the room you would like to choose.");
                        int id = 0;
                        id = scan.nextInt();
                        if (id >= 1 && id <= 360) {
                            try {
                                query = "UPDATE first_hotel_rooms SET availability = false WHERE room_id = ?";
                                pstat = con.prepareStatement(query);
                                pstat.setInt(1, id);
                                pstat.executeUpdate();
                                if ((id >= 301) && (id < 360)) {
                                    System.out.println("You have chosen a luxury room! From you 10,000 $");
                                    System.out.println("Have fun!");
                                    break;
                                } else if ((id >= 121) && (id <= 300)) {
                                    System.out.println("You have chosen a superior room! From you 4,000 $");
                                    System.out.println("Have fun!");
                                    break;
                                } else {
                                    System.out.println("You have chosen a middle class room! From you 1,506 $");
                                    System.out.println("Have fun!");
                                    break;
                                }
                            } catch (Exception e) {
                                e.printStackTrace();
                                break;
                            }
                        }else {
                            System.out.println("You entered the wrong number!");
                            break;
                        }
                    } catch (Exception e) {
                        e.printStackTrace();
                        break;
                    }
                case 3:
                    try {
                        System.out.println("Write your room id: \n");
                        int id = 0;
                        id = scan.nextInt();
                        query = "UPDATE first_hotel_rooms SET availability = true WHERE room_id = ?";
                        pstat = con.prepareStatement(query);
                        pstat.setInt(1, id);
                        pstat.executeUpdate();
                        System.out.println("You have successfully checked out of the room.");
                        break;
                    } catch (Exception e) {
                        e.printStackTrace();
                        break;
                    }
                case 4:
                    try {
                        query = "SELECT restaurant, bar, dining_room FROM catering WHERE hotel_id = 1";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("restaurant:|||   bar:    ||| dining_room:");
                        while (set.next())
                            System.out.println(set.getString(1) + "  ||| " +
                                    set.getString(2) + " |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 5:
                    try {
                        query = "SELECT swimming_pool, sauna, billiards, bowling FROM entertainment WHERE hotel_id = 1";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("swimming_pool:|||   sauna:  ||| billiards: ||| bowling:");
                        while (set.next())
                            System.out.println(set.getString(1) + "     ||| " +
                                    set.getString(2) + " |||  " +
                                    set.getString(3) + " ||| " +
                                    set.getString(4));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 6:
                    try {
                        query = "SELECT daily_maid_service, laundry, dry_cleaning FROM services_availability WHERE hotel_id = 1";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("daily maid service:|||   laundry:    ||| dry cleaning:");
                        while (set.next())
                            System.out.println("     " + set.getString(1) + "     |||  " +
                                    set.getString(2) + "    |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 7:
                    try {
                        query = "SELECT number_of_floors, total_n_of_rooms, rooms_on_floor FROM building_characteristics WHERE hotel_id = 1";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("number of floors: |||total number of rooms: ||| rooms on floor:");
                        while (set.next())
                            System.out.println("     " + set.getString(1) + "   |||  " +
                                    set.getString(2) + "    |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 8:
                    System.out.println("A mid-range room for one night costs $ 1,506;\n" +
                            "A deluxe room for one night costs $ 10,000;\n" +
                            "Superior room for one night class costs $ 4,000;");
                    break;
                case 9:
                    System.out.println("Good buy!");
                    System.exit(0);
                default:
                    System.err.println("Command not found!");
            }
        }
    }
    public String hotel2(){
