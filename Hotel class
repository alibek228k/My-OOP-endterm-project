                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 6:
                    try {
                        query = "SELECT number_of_floors, total_n_of_rooms, rooms_on_floor FROM building_characteristics WHERE hotel_id = 10";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("number of floors: |||total number of rooms: ||| rooms on floor:");
                        while (set.next())
                            System.out.println("     " + set.getString(1) + "   |||  " +
                                    set.getString(2) + "    |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 7:
                    System.out.println("A mid-range room for one night costs $ 150;\n" +
                            "A deluxe room for one night costs $ 650;\n" +
                            "Superior room for one night class costs $ 300;");
                    break;
                case 8:
                    System.out.println("Good buy!");
                    System.exit(0);
                default:
                    System.err.println("Command not found!");
            }
        }
    }
    public String hotel10(){
        con = Client.get_connection();
        while (true) {
            System.out.println("Welcome to Elite Apart-Hotel!\n" +
                    "1.Book a room\n" +
                    "2.Check out the room\n" +
                    "3.Inquire about food at the hotel\n" +
                    "4.Find out about entertainment at the hotel\n" +
                    "5.Find out what services the hotel has\n" +
                    "6.Find out about all rooms\n" +
                    "7.Find out about room prices\n" +
                    "8.Exit.");
            int f = 0;
            f = scan.nextInt();
            switch (f) {
                case 1:
                    try {
                        query = "Select *  from tenth_hotel_rooms \n" +
                                "WHERE availability = true ORDER BY room_id";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        while (set.next())
                            System.out.println(set.getInt("room_id") + " "
                                    + set.getString("availability"));
                        System.out.println("Free rooms are presented above. Write the id of the room you would like to choose.");
                        int id = 0;
                        id = scan.nextInt();
                        if (id >= 1 && id <= 60) {
                            try {
                                query = "UPDATE tenth_hotel_rooms SET availability = false WHERE room_id = ?";
                                pstat = con.prepareStatement(query);
                                pstat.setInt(1, id);
                                pstat.executeUpdate();
                                if ((id >= 41) && (id < 60)) {
                                    System.out.println("You have chosen a luxury room! From you 150 $");
                                    System.out.println("Have fun!");
                                    break;
                                } else if ((id >= 35) && (id <= 40)) {
                                    System.out.println("You have chosen a superior room! From you 100 $");
                                    System.out.println("Have fun!");
                                    break;
                                } else {
                                    System.out.println("You have chosen a middle class room! From you 30 $");
                                    System.out.println("Have fun!");
                                    break;
                                }
                            } catch (Exception e) {
                                e.printStackTrace();
                                break;
                            }
                        } else {
                            System.out.println("You entered the wrong number");
                        }

                    } catch (Exception e) {
                        e.printStackTrace();
                        break;
                    }
                case 2:
                    try {
                        System.out.println("Write your room id: \n");
                        int id = 0;
                        id = scan.nextInt();
                        query = "UPDATE tenth_hotel_rooms SET availability = true WHERE room_id = ?";
                        pstat = con.prepareStatement(query);
                        pstat.setInt(1, id);
                        pstat.executeUpdate();
                        System.out.print("You have successfully checked out of the room.\n");
                        break;
                    } catch (Exception e) {
                        e.printStackTrace();
                        break;
                    }
                case 3:
                    try {
                        query = "SELECT restaurant, bar, dining_room FROM catering WHERE hotel_id = 10";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("restaurant:|||   bar:    ||| dining_room:");
                        while (set.next())
                            System.out.println(set.getString(1) + "  ||| " +
                                    set.getString(2) + " |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 4:
                    try {
                        query = "SELECT swimming_pool, sauna, billiards, bowling FROM entertainment WHERE hotel_id = 10";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("swimming_pool:|||   sauna:  ||| billiards: ||| bowling:");
                        while (set.next())
                            System.out.println(set.getString(1) + "     ||| " +
                                    set.getString(2) + " |||  " +
                                    set.getString(3) + " ||| " +
                                    set.getString(4));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 5:
                    try {
                        query = "SELECT daily_maid_service, laundry, dry_cleaning FROM services_availability WHERE hotel_id = 10";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("daily maid service:|||   laundry:    ||| dry cleaning:");
                        while (set.next())
                            System.out.println("     " + set.getString(1) + "     |||  " +
                                    set.getString(2) + "    |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 6:
                    try {
                        query = "SELECT number_of_floors, total_n_of_rooms, rooms_on_floor FROM building_characteristics WHERE hotel_id = 10";
                        stat = con.createStatement();
                        set = stat.executeQuery(query);
                        System.out.println("number of floors: |||total number of rooms: ||| rooms on floor:");
                        while (set.next())
                            System.out.println("     " + set.getString(1) + "   |||  " +
                                    set.getString(2) + "    |||  " +
                                    set.getString(3));
                        break;
                    } catch (Exception throwables) {
                        throwables.printStackTrace();
                        break;
                    }
                case 7:
                    System.out.println("A mid-range room for one night costs $ 30;\n" +
                            "A deluxe room for one night costs $ 150;\n" +
                            "Superior room for one night class costs $ 100;");
                    break;
